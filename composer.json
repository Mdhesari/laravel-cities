{
  "name": "mdhesari/laravel-cities",
  "description": "Seed all countries/cities from geonames.org database. Searchable DB tree, ready to use API & a bonus vue.js component!",
  "keywords": [
    "laravel",
    "geonames.org",
    "geolocation",
    "countries",
    "cities"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mdhesari/laravel-cities.git",
  "authors": [
    {
      "name": "Giannis Gasteratos",
      "email": "igasteratos@gmail.com"
    },
    {
      "name": "Mohamad Hesari",
      "email": "mdhesari99@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "5.*|^6.0|7.*|8.*"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Mdhesari\\LaravelCities\\": "src/",
      "Mdhesari\\LaravelCities\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.2-dev"
    },
    "laravel": {
      "providers": [
        "Mdhesari\\LaravelCities\\GeoServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
